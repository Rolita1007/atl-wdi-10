<h1>User</h1>

<div>
  <div>ID: {{user.id}}</div>
  <div>First Name: {{user.first_name}}</div>
  <div>Last Name: {{user.last_name}}</div>
  <div>Email: {{user.email}}</div>
  <div>Items:

    <ul>
      {{#each.items}}
        <li>Name: {{name}}</li>
      {{/each}}
    </ul>
  </div>
<div<a href="/edit/{{user.id}}">Edit this User</a>></div>

</div>
